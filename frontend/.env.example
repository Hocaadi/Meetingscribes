# Frontend environment variables
# Copy this file to .env and fill in the values

# Supabase configuration (REQUIRED)
REACT_APP_SUPABASE_URL=https://your-project.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your-anon-key-from-supabase-dashboard

# API Base URL (optional, defaults to localhost:3001)
REACT_APP_API_URL=http://localhost:3001

# Development flags
# NODE_ENV=development is set automatically when using npm start

# API Configuration
REACT_APP_UPLOAD_ENDPOINT=http://localhost:5000/api/upload
REACT_APP_MAX_FILE_SIZE=52428800 